# Environment Variables
# Copy this file to .env and fill in your values

# Cloudinary Configuration
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name_here
EXPO_PUBLIC_CLOUDINARY_API_KEY=your_api_key_here
EXPO_PUBLIC_CLOUDINARY_API_SECRET=your_api_secret_here
EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET=pawspace_transformations

# API Configuration
EXPO_PUBLIC_API_URL=https://api.pawspace.com

# Feature Flags
EXPO_PUBLIC_ENABLE_VIDEO_GENERATION=true
EXPO_PUBLIC_ENABLE_GIF_FALLBACK=true
EXPO_PUBLIC_MAX_VIDEO_DURATION=10
EXPO_PUBLIC_MAX_FILE_SIZE_MB=10
