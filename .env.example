# Public environment variables (bundled in client)

# Supabase (public)
EXPO_PUBLIC_SUPABASE_URL=https://your-project-url.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API (public base URL only)
EXPO_PUBLIC_API_BASE_URL=https://api.pawspace.com
EXPO_PUBLIC_DEV_API_BASE_URL=http://localhost:3000

# App
EXPO_PUBLIC_APP_NAME=PawSpace
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENV=development

# Stripe (publishable key is public by design)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_PREMIUM_FEATURES=false
EXPO_PUBLIC_ENABLE_VIDEO_TRANSFORMATIONS=false
EXPO_PUBLIC_ENABLE_LIVE_CHAT=false

# Maps & Analytics
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
EXPO_PUBLIC_GOOGLE_ANALYTICS_ID=

# Cloudinary (client-safe values only; DO NOT ship api_secret)
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
EXPO_PUBLIC_CLOUDINARY_API_KEY=your-cloudinary-api-key

# Development
EXPO_PUBLIC_DEBUG_MODE=false
