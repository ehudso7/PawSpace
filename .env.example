# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project-url.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.pawspace.com
EXPO_PUBLIC_DEV_API_BASE_URL=http://localhost:3000

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_APP_VERSION=1.0.0

# Analytics
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Push Notifications
EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED=true

# Social Media
EXPO_PUBLIC_INSTAGRAM_APP_ID=your-instagram-app-id
EXPO_PUBLIC_FACEBOOK_APP_ID=your-facebook-app-id

# Payment Processing
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Map Services
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Image Processing
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
EXPO_PUBLIC_CLOUDINARY_API_KEY=your-cloudinary-api-key

# Feature Flags
EXPO_PUBLIC_ENABLE_PREMIUM_FEATURES=true
EXPO_PUBLIC_ENABLE_VIDEO_TRANSFORMATIONS=false
EXPO_PUBLIC_ENABLE_LIVE_CHAT=true

# Development
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_ENABLE_FLIPPER=true